@model List<Category>

<h1>Category</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Category Name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(o => o.DisplayOrder))
        {
            <tr>
                <td>@obj.Id</td>
                <td>@obj.Name</td>
            </tr>
        }
    </tbody>


</table>